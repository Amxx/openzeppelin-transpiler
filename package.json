{
  "name": "@openzeppelin/upgrade-safe-transpiler",
  "version": "0.1.0",
  "description": "Solidity preprocessor used to generate OpenZeppelin Contracts Upgrade Safe.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!*.tsbuildinfo"
  ],
  "bin": {
    "upgrade-safe-transpiler": "dist/cli.js"
  },
  "scripts": {
    "test": "tsc && buidler compile && ava",
    "test:watch": "fgbg 'buidler compile && ava -w' 'tsc -w' --",
    "compile:ts": "rm -rf lib && tsc",
    "prepare": "npm run compile:ts",
    "watch": "tsc -w",
    "lint": "eslint . --ignore-path .gitignore --max-warnings 0"
  },
  "repository": "github:OpenZeppelin/openzeppelin-transpiler",
  "keywords": [
    "solidity",
    "openzeppelin",
    "smart-contarcts",
    "ethereum",
    "upgradeability",
    "openzeppelin-sdk"
  ],
  "author": "Igor Yalovoy <igor@openzeppelin.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenZeppelin/openzeppelin-sdk/issues"
  },
  "homepage": "https://github.com/OpenZeppelin/openzeppelin-sdk#readme",
  "dependencies": {
    "ajv": "^6.10.2",
    "lodash": "^4.17.15",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.5",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@nomiclabs/buidler": "^1.4.8",
    "@openzeppelin/contracts": "^2.4.0",
    "@types/lodash": "^4.14.149",
    "@types/minimatch": "^3.0.3",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "ava": "^3.13.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fgbg": "^0.1.5",
    "fs-extra": "^9.0.0",
    "prettier": "^1.19.1",
    "solidity-ast": "^0.4.13",
    "typescript": "^4.0.0"
  }
}
